    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "handlebars": "^4.7.8",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^10.1.3",
    "sass-embedded": "^1.83.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.5",
    "vite-plugin-checker": "latest",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
