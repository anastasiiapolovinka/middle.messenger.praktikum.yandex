{
  "name": "polovinka_messanger",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.11.0"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "tsc --noEmit",
    "build": "npm run prebuild && vite build",
